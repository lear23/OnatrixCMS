@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
    Layout = "mainLayout.cshtml";
    ViewBag.Title = Model?.Value("pageTitle") ?? null;

    var isContactPage = Umbraco.AssignedContentItem.ContentType.Alias == "contactPage";
    ViewData["IsContactPage"] = isContactPage;
    

    bool.TryParse(ViewData["error_name"]?.ToString()!, out bool error_name);
    bool.TryParse(ViewData["error_email"]?.ToString()!, out bool error_email);
    bool.TryParse(ViewData["error_phone"]?.ToString()!, out bool error_phone);
    bool.TryParse(ViewData["error_service"]?.ToString()!, out bool error_service);

}



<main>


    @await Html.PartialAsync("static/NextProject")
 

    <section class="contactSection">

        <div class="map-container">

            <div id="map">
                <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d16188.303452309192!2d17.9347542!3d59.52412915!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1ssv!2sse!4v1726908925282!5m2!1ssv!2sse"
                        width="100%" height="1200px" style="border:0;"
                        allowfullscreen="" loading="lazy"
                        referrerpolicy="no-referrer-when-downgrade">
                </iframe>
            </div>
        </div>
        <div id="myform" class="container contact">
           
            <h4>Request a call back</h4>
            <h1>Let us know about your <br> next project</h1>
            <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Maecenas consectetur, ligula semper sagittis lobortis, eros metus efficitur purus</p>

            <div id="myForm" class="form-container-contact">

                @await Html.PartialAsync("static/contactForm", new OnatrixCMS.ViewModels.ContactFormViewModel())
            </div>

         </div>      
     


    </section>
    <section class="container logos">
        @await Html.PartialAsync("static/LogoBusiness")
    </section>

</main>

